{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx": "react-jsx",

    "module": "commonjs",

    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,

    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include":["src"]
}
